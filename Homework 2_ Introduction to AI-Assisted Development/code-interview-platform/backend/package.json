{
    "name": "code-interview-backend",
    "version": "1.0.0",
    "description": "WebSocket server for real-time collaborative coding interviews",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "node --watch server.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "keywords": [
        "websocket",
        "collaboration",
        "interview",
        "real-time"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "ws": "^8.14.2",
        "nanoid": "^5.0.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "@types/jest": "^29.5.11"
    }
}